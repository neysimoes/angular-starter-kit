  "devDependencies": {
    "bootstrap-styl": "^5.0.5",
    "bower": "^1.7.7",
    "browser-sync": "^2.11.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-eslint": "^2.0.0",
    "gulp-html-replace": "^1.5.5",
    "gulp-livereload": "^3.8.1",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-postcss": "^6.1.0",
    "gulp-stylus": "^2.3.0",
    "gulp-uglify": "^1.5.3",
    "lost": "^6.7.2",
    "npm-run-all": "^1.5.2",
    "rucksack-css": "^0.8.5",
    "rupture": "^0.6.1",
    "wiredep": "^3.0.0"
  },
  "eslint": {
    "angular": {
      "rules": {
        "quotes": [
          2,
          "single"
        ],
        "linebreak-style": [
          2,
          "unix"
        ],
        "semi": [
          2,
          "always"
        ],
        "angular/controller-name": 1,
        "angular/typecheck-number": 1,
        "angular/typecheck-object": 1,
        "angular/typecheck-function": 1,
        "angular/typecheck-array": 1
      },
      "env": {
        "es6": true,
        "node": false,
        "browser": true
      },
      "extends": "angular"
    }
  },
  "wiredep": {
    "ignorePath": "app/",
    "exclude": "angular-mocks",
    "overrides": {
      "font-awesome": {
        "main": [
          "./css/font-awesome.css",
          "./fonts/*"
        ]
      },
      "bootstrap": {
        "main": [
          "dist/js/bootstrap.js"
        ]
      }
    }
  }
}
